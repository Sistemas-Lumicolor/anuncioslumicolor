<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="group_allow_send_produce" model="res.groups">
        <field name="name">Allow to send to produce</field>
        <field name="category_id" eval="ref('anuncioslumicolor.module_category_anuncioslumicolor')" />
        <field name="comment">It allows to show button to send to produce</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
    </record>
    <record id="group_allow_sale_products" model="res.groups">
        <field name="name">Allow to sale products</field>
        <field name="category_id" eval="ref('anuncioslumicolor.module_category_anuncioslumicolor')" />
        <field name="comment">It allows to sale products for any price</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
    </record>

    <record id="group_allow_see_all_contacts" model="res.groups">
        <field name="name">Allow to see all contacts</field>
        <field name="category_id" eval="ref('anuncioslumicolor.module_category_anuncioslumicolor')" />
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
    </record>
</odoo>
